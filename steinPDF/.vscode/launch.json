{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Electron",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "program": "${workspaceFolder}/src/main.js",
      "preLaunchTask": "npm: start",
      "console": "integratedTerminal"
    }
  ]
}